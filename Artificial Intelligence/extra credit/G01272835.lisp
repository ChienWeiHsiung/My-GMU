;;
;;transform data into required format 
(defun transform (examples feature-defs ) 
  (let ((data '()))
    (loop for example in examples
	  Do (setf data (append data (list (append (mapcar #'(lambda (x y) (list (car y) x)) (car example) feature-defs) (list (list :label (car (car (cdr example)))))))))
	  )
    data))

;;Compute accuracy from two lists. Ex : (1 1 0) (0 1 1) => 1/3
(defun accuracy (a b)
  (float (/ (apply '+ (mapcar #'(lambda (x y) (if (eq x y) 1 0)) a b)) (length a)))
  )

